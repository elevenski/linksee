<title><%- name %> (@<%- nickname %>) 's Profile | Linksee</title>
<meta property="og:title" content="<%- name %> (@<%- nickname %>)">
<meta name="og:description" content="See <%- name %> (@<%- nickname %>) 's Linksee Profile">
<meta property="og:image" content="<%- avatar %>">
<meta name="theme-color" content="#059de1">
<% include ./partials/header %>

<body>

    <div class="this-is-section">
        <div class="user mt-3 mb-3">
            <center><img draggable="false" class="rounded-full user-avatar" width="100" height="100"
                    src="<%- avatar && avatar ? avatar : 'data:image/png;base64,' %>"
                    alt="User" onerror="this.onerror=null;this.src='data:image/png;base64,'"
              ></center>
            <p class="user-name mt-3"><%- name %> <% if(verified === true) {%><i style="color:#059de1" class="fa-solid fa-circle-check"></i><% } %></p>
            <p class="user-nickname mt-1">@<%- nickname %></p>
            <p class="user-bio mt-2"><%- bio %></p>
        </div>
        <div class="soial mt-3">
            <% if(website) { %>
            <a class="social-btn -website" href="<%- website%>" target="_blank"><i class="fa-solid fa-globe"></i></a>
            <%}%>
            <% if(facebook) { %>
            <a class="social-btn -facebook" href="https://facebook.com/<%- facebook%>" target="_blank"><i class="fa-brands fa-facebook"></i></a>
            <%}%>
            <% if(twitter) { %>
            <a class="social-btn -twitter" href="https://twitter.com/<%- twitter%>" target="_blank"><i class="fa-brands fa-twitter"></i></a>
            <%}%>
            <% if(instagram) { %>
            <a class="social-btn -instagram" href="https://instagram.com/<%- instagram%>" target="_blank"><i class="fa-brands fa-instagram"></i></a>
            <%}%> 
            <% if(youtube) { %>
            <a class="social-btn -youtube" href="https://youtube.com/channel/<%- youtube%>" target="_blank"><i class="fa-brands fa-youtube"></i></a>
            <%}%> 
            <% if(github) { %>
            <a class="social-btn -github" href="https://github.com/<%- github%>" target="_blank"><i class="fa-brands fa-github"></i></a>
            <%}%>
            <% if(discord) { %>
            <a class="social-btn -discord" href="https://discord.com/users/<%- discord%>" target="_blank"><i class="fa-brands fa-discord"></i></a>
            <%}%>
            <% if(linkedin) { %>
            <a class="social-btn -linkedin" href="https://linkedin.com/in/<%- linkedin%>" target="_blank"><i class="fa-brands fa-linkedin"></i></a>
            <%}%>
            <% if(twitch) { %>
            <a class="social-btn -twitch" href="https://twitch.tv/<%- twitch%>" target="_blank"><i class="fa-brands fa-twitch"></i></a>
            <%}%>
            <% if(mail) { %>
            <a class="social-btn -mail" href="mailto:<%- mail%>"><i class="fa-regular fa-at"></i></a>
            <%}%>
        </div>
        <div class="links mt-3">
            <% cards.forEach(function(card){ %>
              <a class="links-btn" href="<%- card.link || "#"%>" target="_blank"><%- card.title || "Card Title" %></a>
            <% }) %>  
        </div>
              <br>
        <!--<% include ./partials/footer %>-->
    </div>

</body>

</html>